		"express": "~4.16.1",
		"feather-icons": "4.25.x",
		"fs-extra": "^10.0.0",
		"hbs": "~4.0.4",
		"http-errors": "~1.6.3",
		"jquery": "3.5.x",
		"level": "^6.0.1",
		"morgan": "~1.9.1",
		"popper.js": "1.16.x",
